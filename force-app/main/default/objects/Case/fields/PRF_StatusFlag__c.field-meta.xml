<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PRF_StatusFlag__c</fullName>
    <externalId>false</externalId>
    <formula>IMAGE( 
CASE(  PRF_Status__c , 
"Approved", "/img/samples/flag_green.gif",
"Draft", "/img/samples/flag_yellow.gif",
"Submitted", "/img/samples/flag_yellow.gif",
"Rejected", "/img/samples/flag_red.gif", 
"Cancelled", "/img/samples/flag_red.gif", 
"/s.gif"), 
"Status Flag")</formula>
    <label>PRF Status Flag</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
